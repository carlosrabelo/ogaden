{
    "python.envFile": "",
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnSave": true,
    "autopep8.args": [
        "--max-line-length",
        "200"
    ],
    "black-formatter.args": [
        "--line-length",
        "200",
        "-a"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/.venv": true
    }
}